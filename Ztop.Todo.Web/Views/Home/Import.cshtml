@{
    Dictionary<string, List<Ztop.Todo.ActiveDirectory.AUser>> data = ViewBag.Data;
}
<form action="Import" method="post">
    @foreach (var kv in data)
    {
        var group = kv.Key;
        var users = kv.Value;

        <div class="form-group">
            <input type="hidden" name="Groups" value="@(group)" />@(group)
            <div>
                @foreach (var user in users)
                {
                    <label class="control-label"><input type="checkbox" checked="checked" class="checkbox-inline" name="Users" value="@(group)_@(user.Account)_@(user.Name)" />@(user.Name)</label>
                }
            </div>
        </div>
    }
    <div class="form-group">
        <button type="submit" class="btn btn-primary">提交</button>
    </div>
</form>