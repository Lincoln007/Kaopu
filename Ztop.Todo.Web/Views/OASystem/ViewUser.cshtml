@{ 
    List<User> users = ViewBag.User;
    var arrays = users.ListToTable(4);
    Layout = null;
}

<div class="page-header">
    <h3 class="text-center">用户列表</h3>
</div>

<table class="table">
    @foreach(var array in arrays)
    {
        <tr>
            @foreach(var item in array)
            {
                if (item != null)
                {
                    <td>@(item.DisplayName)</td>
                }
            }
        </tr>
    }
</table>
