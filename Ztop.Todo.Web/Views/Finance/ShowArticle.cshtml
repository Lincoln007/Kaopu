@model Ztop.Todo.Model.Article

<table class="table table-striped table-bordered table-condensed">
    <tbody>
        <tr>
            <th>项目名称</th>
            <td>@(Model.Name)</td>
            <th>登记编号</th>
            <td>@(Model.Number)</td>
        </tr>
        <tr>
            <th>对方单位</th>
            <td>@(Model.OtherSide)</td>
            <th>意向金额【元】</th>
            <td>@(Model.Money)</td>
        </tr>
        <tr>
            <th>所属城市</th>
            <td>@(Model.City)</td>
            <th>项目类型</th>
            <td>@(Model.ProjectType)</td>
        </tr>
        <tr>
            <th>备注</th>
            <td>@(Model.Remark)</td>
            <th>登记编号</th>
            <td></td>
        </tr>
    </tbody>
</table>
